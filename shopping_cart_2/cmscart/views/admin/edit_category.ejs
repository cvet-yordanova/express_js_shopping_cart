<%- include('../_layouts/adminheader') %>

<h2 class="page-title">Edit a category</h2>
<a href="/admin/categories" class="btn btn-primary">Back to all categories</a>
<br><br>


<form action="/admin/categories/edit-category/<%= id %>" method="post">

    <div class="form-group">
        <label for="">Category</label>
        <input type="text" class="form-control" name="title"  placeholder="title" value="<%= title %>">
    </div>

    <input type="submit">

</form>

<%- include('../_layouts/adminfooter')%>